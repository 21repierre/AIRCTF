<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>logo_air_small.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="pictures\logo_air.png" />
      <Resource Include="pictures\logo_air.png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>

</Project>
